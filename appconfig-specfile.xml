  <presentation defaultLocale="en-US">
    <field keyName="configVersion" type="hidden">
    </field>
    <fieldGroup>
      <name>
        <language value="en-US">Licensing</language>
      </name>
      <field keyName="license" type="input">
        <label>
          <language value="en-US">License key</language>
        </label>
        <description>
          <language value="en-US">Contact sales@keepassium.com to get one.</language>
        </description>
      </field>
      <field keyName="licenseSeatID" type="select">
        <label>
          <language value="en-US">License seat identifier</language>
        </label>
        <description>
          <language value="en-US">Associates the license to the specific device or user. Select device-based and user-based variable as appropriate for your environment.</language>
        </description>
        <options>
          <option value="none">
            <language value="en-US">None (site license)</language>
          </option>
          <option value="${userUID}" selected="true">
            <language value="en-US">Ivanti: User login ID</language>
          </option>
          <option value="${deviceSN}">
            <language value="en-US">Ivanti: Device's serial number</language>
          </option>
          <option value="$SERIALNUMBER">
            <language value="en-US">JAMF Pro: Device's serial number</language>
          </option>
          <option value="$USERNAME">
            <language value="en-US">JAMF Pro: Username</language>
          </option>
          <option value="%UserId%">
            <language value="en-US">JAMF School: User's internal ID</language>
          </option>
          <option value="%SerialNumber%">
            <language value="en-US">JAMF School: Deviceâ€™s serial number</language>
          </option>
          <option value="%serialNumber%">
            <language value="en-US">MaaS360: Device's serial number</language>
          </option>
          <option value="{{serial_number}}">
            <language value="en-US">SimpleMDM: Device's serial number</language>
          </option>
          <option value="{{accountid}}" selected="true">
            <language value="en-US">Intune: User account ID</language>
          </option>
          <option value="{{serialnumberlast4digits}}">
            <language value="en-US">Intune: Device serial number, last 4 digits</language>
          </option>
          <option value="{{deviceid}}">
            <language value="en-US">Intune: Device ID</language>
          </option>
        </options>
      </field>
      <field keyName="supportEmail" type="input">
        <label>
          <language value="en-US">Support email</language>
        </label>
        <description>
          <language value="en-US">Overrides the default support email address in the app. Enter email of your company's in-house support team.</language>
        </description>
      </field>
    </fieldGroup>
    <fieldGroup>
      <name>
        <language value="en-US">App and data protection</language>
      </name>
      <field keyName="allowAppProtection" type="select">
        <label>
          <language value="en-US">Disable this to hide App Protection from onboarding and app settings. This is useful if PIN/biometric check is enforced by your MDM.</language>
        </label>
        <description>
          <language value="en-US"></language>
        </description>
        <options>
          <option value="">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="requireAppPasscodeSet" type="select">
        <label>
          <language value="en-US">Require App Passcode</language>
        </label>
        <description>
          <language value="en-US">Require that the user configures an app protection passcode. This is enforced by KeePassium, independently from PIN access that might be enforced by your MDM or the system.</language>
        </description>
        <options>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="minimumAppPasscodeEntropy" type="select">
        <label>
          <language value="en-US">Minimal complexity of App Passcode</language>
        </label>
        <description>
          <language value="en-US">Minimal required complexity for user-defined app passcode. Entropy is estimated by Zxcvbn library. </language>
        </description>
        <options>
          <option value="0">
            <language value="en-US">0 bits (accept everything)</language>
          </option>
          <option value="10">
            <language value="en-US">10 bits (a non-trivial 4-digit PIN)</language>
          </option>
          <option value="20">
            <language value="en-US">20 bits (a non-trivial 6-digit PIN)</language>
          </option>
          <option value="30">
            <language value="en-US">30 bits (approx. 8 letters)</language>
          </option>
          <option value="40">
            <language value="en-US">40 bits (approx. 8 upper+lower+digits)</language>
          </option>
        </options>
      </field>
      <field keyName="minimumAppPasscodeLength" type="input">
        <label>
          <language value="en-US">Minimal length of App Passcode</language>
        </label>
        <description>
          <language value="en-US">Is enforced together with minimal complexity.</language>
        </description>
      </field>
      <field keyName="minimumDatabasePasswordEntropy" type="select">
        <label>
          <language value="en-US">Minimal complexity of Database Passwords</language>
        </label>
        <description>
          <language value="en-US">Minimal required complexity for user-defined database passwords. Is enforced on database creation and when changing the master key. Less complex passwords will be rejected. Entropy is estimated by Zxcvbn library. </language>
        </description>
        <options>
          <option value="0">
            <language value="en-US">0 bits (accept everything)</language>
          </option>
          <option value="10">
            <language value="en-US">10 bits (a non-trivial 4-digit PIN)</language>
          </option>
          <option value="20">
            <language value="en-US">20 bits (a non-trivial 6-digit PIN)</language>
          </option>
          <option value="30">
            <language value="en-US">30 bits (approx. 8 letters)</language>
          </option>
          <option value="40">
            <language value="en-US">40 bits (approx. 8 upper+lower+digits)</language>
          </option>
          <option value="60">
            <language value="en-US">60 bits</language>
          </option>
          <option value="80">
            <language value="en-US">80 bits</language>
          </option>
          <option value="100">
            <language value="en-US">100 bits</language>
          </option>
        </options>
      </field>
      <field keyName="minimumDatabasePasswordLength" type="input">
        <label>
          <language value="en-US">Minimal length of Database Passwords</language>
        </label>
        <description>
          <language value="en-US">Is enforced together with minimal complexity.</language>
        </description>
      </field>
      <field keyName="protectKeyFileInput" type="select">
        <label>
          <language value="en-US">Protect key file input field</language>
        </label>
        <description>
          <language value="en-US">Hide the name of selected key file behind dots, like a password.</language>
        </description>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="allowDatabaseEncryptionSettings" type="select">
        <label>
          <language value="en-US">Allow viewing/editing database encryption settings</language>
        </label>
        <description>
          <language value="en-US"></language>
        </description>
        <options>
          <option value="">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="allowDatabasePrint" type="select">
        <label>
          <language value="en-US">Allow printing complete database</language>
        </label>
        <description>
          <language value="en-US">This parameter controls the "Print" menu in database viewer. Users still can select text and export it to a printer app via context menu.</language>
        </description>
        <options>
          <option value="">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
    </fieldGroup>

    <fieldGroup>
      <name>
        <language value="en-US">App settings</language>
      </name>
      <field keyName="autoUnlockLastDatabase" type="select">
        <label>
          <language value="en-US">Auto-unlock last used database</language>
        </label>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="rememberDatabaseKey" type="select">
        <label>
          <language value="en-US">Remember master keys</language>
        </label>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="rememberDatabaseFinalKey" type="select">
        <label>
          <language value="en-US">Skip YubiKey scan when possible</language>
        </label>
        <description>
          <language value="en-US">Sometimes KeePassium can cache database encryption keys and not require YubiKey scan. This setting controls whether such a behavior is allowed, or YubiKey scans must be enforced.</language>
        </description>
        <options>
          <option value="">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Skip when possible</language>
          </option>
          <option value="no">
            <language value="en-US">Always require</language>
          </option>
        </options>
      </field>
      <field keyName="keepKeyFileAssociations" type="select">
        <label>
          <language value="en-US">Remember the last used key file of each database</language>
        </label>
        <description>
          <language value="en-US">Remember and automatically select key files last used with each database.</language>
        </description>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="keepHardwareKeyAssociations" type="select">
        <label>
          <language value="en-US">Remember the last used hardware key of each database</language>
        </label>
        <description>
          <language value="en-US">Remember and automatically select hardware keys last used with each database.</language>
        </description>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="lockAllDatabasesOnFailedPasscode" type="select">
        <label>
          <language value="en-US">Lock databases after a failed App Protection passcode</language>
        </label>
        <description>
          <language value="en-US">If the user enters an incorrect App Protection passcode once, the app would lock all the databases and clear any remembered master keys from keychain.</language>
        </description>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="appLockTimeout" type="select">
        <label>
          <language value="en-US">App protection timeout</language>
        </label>
        <description>
          <language value="en-US">If not configured, the app will let the user decide.</language>
        </description>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="0">
            <language value="en-US">Immediately when leaving the app</language>
          </option>
          <option value="3">
            <language value="en-US">After 3 seconds</language>
          </option>
          <option value="15">
            <language value="en-US">After 15 seconds</language>
          </option>
          <option value="30">
            <language value="en-US">After 30 seconds</language>
          </option>
          <option value="60">
            <language value="en-US">After 1 minute</language>
          </option>
          <option value="120">
            <language value="en-US">After 2 minutes</language>
          </option>
          <option value="300">
            <language value="en-US">After 5 minutes</language>
          </option>
        </options>
      </field>
      <field keyName="lockAppOnLaunch" type="select">
        <label>
          <language value="en-US">Lock app on launch</language>
        </label>
        <description>
          <language value="en-US">Enforces app protection when the app is first launched (after device restart), even if appLockTimeout has not expired yet.</language>
        </description>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="lockDatabasesOnReboot" type="select">
        <label>
          <language value="en-US">Lock databases on device restart</language>
        </label>
        <description>
          <language value="en-US">Trigger database lock timeout after device reboot.</language>
        </description>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="databaseLockTimeout" type="select">
        <label>
          <language value="en-US">Database lock timeout</language>
        </label>
        <description>
          <language value="en-US">Leave not configured to let the user decide.</language>
        </description>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="0">
            <language value="en-US">Immediately</language>
          </option>
          <option value="5">
            <language value="en-US">After 5 seconds</language>
          </option>
          <option value="15">
            <language value="en-US">After 15 seconds</language>
          </option>
          <option value="30">
            <language value="en-US">After 30 seconds</language>
          </option>
          <option value="60">
            <language value="en-US">After 1 minute</language>
          </option>
          <option value="120">
            <language value="en-US">After 2 minutes</language>
          </option>
          <option value="300">
            <language value="en-US">After 5 minutes</language>
          </option>
          <option value="600">
            <language value="en-US">After 10 minutes</language>
          </option>
          <option value="1800">
            <language value="en-US">After 30 minutes</language>
          </option>
          <option value="3600">
            <language value="en-US">After 1 hour</language>
          </option>
          <option value="7200">
            <language value="en-US">After 2 hours</language>
          </option>
          <option value="14400">
            <language value="en-US">After 4 hours</language>
          </option>
          <option value="28800">
            <language value="en-US">After 8 hours</language>
          </option>
          <option value="86400">
            <language value="en-US">After 24 hours</language>
          </option>
          <option value="172800">
            <language value="en-US">After 48 hours</language>
          </option>
          <option value="604800">
            <language value="en-US">After 7 days</language>
          </option>
          <option value="-1">
            <language value="en-US">Never</language>
          </option>
        </options>
      </field>
      <field keyName="clipboardTimeout" type="select">
        <label>
          <language value="en-US">Clipboard timeout</language>
        </label>
        <description>
          <language value="en-US">Cleans up any values copied to clipboard after the configured time.</language>
        </description>
        <options>
          <option value="" selected="true">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="0">
            <language value="en-US">Disable cut/copy to clipboard</language>
          </option>
          <option value="10">
            <language value="en-US">After 10 seconds</language>
          </option>
          <option value="20">
            <language value="en-US">After 20 seconds</language>
          </option>
          <option value="30">
            <language value="en-US">After 30 seconds</language>
          </option>
          <option value="60">
            <language value="en-US">After 1 minute</language>
          </option>
          <option value="120">
            <language value="en-US">After 2 minutes</language>
          </option>
          <option value="180">
            <language value="en-US">After 3 minutes</language>
          </option>
          <option value="300">
            <language value="en-US">After 5 minutes</language>
          </option>
          <option value="600">
            <language value="en-US">After 10 minutes</language>
          </option>
          <option value="1200">
            <language value="en-US">After 20 minutes</language>
          </option>
        </options>
      </field>
      <field keyName="useUniversalClipboard" type="select">
        <label>
          <language value="en-US">Use universal clipboard</language>
        </label>
        <description>
          <language value="en-US">Allows pasting to other Apple devices. Relies on iCloud infrastructure. Has a non-configurable timeout of about 2 minutes, predefined by Apple.</language>
        </description>
        <options>
          <option value="">
            <language value="en-US">(Not configured)</language>
          </option>
          <option value="yes">
            <language value="en-US">Yes</language>
          </option>
          <option value="no">
            <language value="en-US">No</language>
          </option>
        </options>
      </field>
      <field keyName="hideProtectedFields" type="select">
        <label>
          <language value="en-US">Hide protected fields</language>
        </label>
        <description>
